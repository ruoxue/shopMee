<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.religion.mapper.ReligionPartyManagerMapper">
    
    <resultMap type="ReligionPartyManager" id="ReligionPartyManagerResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="tenantId"    column="tenant_id"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="status"    column="status"    />
        <result property="deleted"    column="deleted"    />
        <result property="type"    column="type"    />
        <result property="remake"    column="remake"    />
    </resultMap>

    <sql id="selectReligionPartyManagerVo">
        select id, name, tenant_id, create_by, create_time, update_by, update_time, status, deleted, type, remake from religion_party_manager
    </sql>

    <select id="selectReligionPartyManagerList" parameterType="ReligionPartyManager" resultMap="ReligionPartyManagerResult">
        <include refid="selectReligionPartyManagerVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="type != null  and type != ''"> and type = #{type}</if>
            <if test="remake != null  and remake != ''"> and remake = #{remake}</if>
        </where>
    </select>
    
    <select id="selectReligionPartyManagerById" parameterType="Long" resultMap="ReligionPartyManagerResult">
        <include refid="selectReligionPartyManagerVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertReligionPartyManager" parameterType="ReligionPartyManager" useGeneratedKeys="true" keyProperty="id">
        insert into religion_party_manager
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">name,</if>
            <if test="tenantId != null">tenant_id,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="status != null">status,</if>
            <if test="deleted != null">deleted,</if>
            <if test="type != null">type,</if>
            <if test="remake != null">remake,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">#{name},</if>
            <if test="tenantId != null">#{tenantId},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="status != null">#{status},</if>
            <if test="deleted != null">#{deleted},</if>
            <if test="type != null">#{type},</if>
            <if test="remake != null">#{remake},</if>
         </trim>
    </insert>

    <update id="updateReligionPartyManager" parameterType="ReligionPartyManager">
        update religion_party_manager
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="tenantId != null">tenant_id = #{tenantId},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="status != null">status = #{status},</if>
            <if test="deleted != null">deleted = #{deleted},</if>
            <if test="type != null">type = #{type},</if>
            <if test="remake != null">remake = #{remake},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteReligionPartyManagerById" parameterType="Long">
        delete from religion_party_manager where id = #{id}
    </delete>

    <delete id="deleteReligionPartyManagerByIds" parameterType="String">
        delete from religion_party_manager where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>